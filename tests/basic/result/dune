  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
)
(alias (name "ptests_config")
  (deps (alias "test_structs.diff"))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
)
; TEST FILE "test_array.c"
(alias (name "test_array.wtests")
  (deps (alias "test_array.0.exec.wtests"))) ; to performs all sub-tests related to a file
(alias (name "test_array.c")
  (deps (alias "test_array.0.exec"))) ; to reproduce and visualize the all sub-test outputs related to a file
(rule ; TEST #0 OF TEST FILE tests/basic/test_array.c
  (alias "test_array.0.exec.wtests")
  (targets "test_array.err.log" "test_array.res.log"  )
  (deps "test_array.0.exec.wtests" "../oracle/test_array.res.oracle" "../oracle/test_array.err.oracle"  "test_array.c"  (package frama-c)  (package "frama-c-pluscal") )
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (run frama-c-wtests -brief "test_array.0.exec.wtests" "frama-c -check -no-autoload-plugins -add-symbolic-path=\"/home/amira/dev/c2pluscal/_build/default:.\" -load-plugin=pluscal    test_array.c -pluscal  > \"test_array.res.log\" 2> \"test_array.err.log\"" ))
)
(rule ; REPRODUCE TEST #0 OF TEST FILE "test_array.c"
  (alias "test_array.0.exec")
  (deps  "test_array.c"  (package frama-c)  (package "frama-c-pluscal")  (universe))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (with-accepted-exit-codes 0 (system "frama-c -check -no-autoload-plugins -add-symbolic-path=\"/home/amira/dev/c2pluscal/_build/default:.\" -load-plugin=pluscal    test_array.c -pluscal ")))
)
(rule ; SHOW TEST COMMAND #0 OF TEST FILE "test_array.c"
  (alias "test_array.0.exec.show")
  (deps  "test_array.c"  (package frama-c)  (package "frama-c-pluscal")  (universe))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (system "echo 'frama-c -check -no-autoload-plugins -add-symbolic-path=\"/home/amira/dev/c2pluscal/_build/default:.\" -load-plugin=pluscal    test_array.c -pluscal  > \"test_array.res.log\" 2> \"test_array.err.log\"'"))
)
(rule
  (alias "test_array.0.exec.diff")
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (diff "../oracle/test_array.res.oracle" "test_array.res.log"))
)
(rule
  (alias "test_array.0.exec.diff")
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (diff "../oracle/test_array.err.oracle" "test_array.err.log"))
)
(alias (name "test_array.diff")
  (deps (alias "test_array.0.exec.diff"))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
)
(alias (name "ptests_config")
  (deps (alias "test_array.diff"))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
)
