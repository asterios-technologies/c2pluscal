(alias (name disabled_ptests)
  (deps (alias disabled_ptests_config)))
(alias (name ptests)
  (deps (alias disabled_ptests)))
(alias (name ptests_config)
  (deps (alias disabled_ptests_config)))
(rule ; Warns when some test conditions are disabled
  (alias disabled_ptests_config)
  (deps (universe))
  (enabled_if (or false  (not %{lib-available:frama-c-pluscal.core})))
  (action (progn (echo "WARNING: Enabling conditions of some tests are false for @tests/basic/ptests_config\n")
                 (echo "- \%{lib-available:frama-c-pluscal.core}: " %{lib-available:frama-c-pluscal.core} "\n")
  ))
)

