(copy_files ../*.*)
(alias (name ptests)
  (deps (alias ptests_config)))
; TEST FILE "test_local_var.c"
(alias (name "test_local_var.wtests")
  (deps (alias "test_local_var.0.exec.wtests"))) ; to performs all sub-tests related to a file
(alias (name "test_local_var.c")
  (deps (alias "test_local_var.0.exec"))) ; to reproduce and visualize the all sub-test outputs related to a file
(rule ; TEST #0 OF TEST FILE tests/basic/test_local_var.c
  (alias "test_local_var.0.exec.wtests")
  (targets "test_local_var.err.log" "test_local_var.res.log"  )
  (deps "test_local_var.0.exec.wtests" "../oracle/test_local_var.res.oracle" "../oracle/test_local_var.err.oracle"  "test_local_var.c"  (package frama-c)  (package "frama-c-pluscal") )
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (run frama-c-wtests -brief "test_local_var.0.exec.wtests" "frama-c -check -no-autoload-plugins -add-symbolic-path=\"/home/amira/dev/c2pluscal/_build/default:.\" -load-plugin=pluscal    test_local_var.c -pluscal  > \"test_local_var.res.log\" 2> \"test_local_var.err.log\"" ))
)
(rule ; REPRODUCE TEST #0 OF TEST FILE "test_local_var.c"
  (alias "test_local_var.0.exec")
  (deps  "test_local_var.c"  (package frama-c)  (package "frama-c-pluscal")  (universe))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (with-accepted-exit-codes 0 (system "frama-c -check -no-autoload-plugins -add-symbolic-path=\"/home/amira/dev/c2pluscal/_build/default:.\" -load-plugin=pluscal    test_local_var.c -pluscal ")))
)
(rule ; SHOW TEST COMMAND #0 OF TEST FILE "test_local_var.c"
  (alias "test_local_var.0.exec.show")
  (deps  "test_local_var.c"  (package frama-c)  (package "frama-c-pluscal")  (universe))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (system "echo 'frama-c -check -no-autoload-plugins -add-symbolic-path=\"/home/amira/dev/c2pluscal/_build/default:.\" -load-plugin=pluscal    test_local_var.c -pluscal  > \"test_local_var.res.log\" 2> \"test_local_var.err.log\"'"))
)
(rule
  (alias "test_local_var.0.exec.diff")
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (diff "../oracle/test_local_var.res.oracle" "test_local_var.res.log"))
)
(rule
  (alias "test_local_var.0.exec.diff")
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
(action (diff "../oracle/test_local_var.err.oracle" "test_local_var.err.log"))
)
(alias (name "test_local_var.diff")
  (deps (alias "test_local_var.0.exec.diff"))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
)
(alias (name "ptests_config")
  (deps (alias "test_local_var.diff"))
  (enabled_if (and true %{lib-available:frama-c-pluscal.core}))
)
